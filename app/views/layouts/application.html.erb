<!DOCTYPE html>
<html>
<head>
  <title>Beansbeansbeans</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body class="<%= params[:controller] %>">
  <nav class="navbar navbar-default" role="navigation">
<div class="container-fluid">
  <%= link_to "Home", root_url %>
<div class="navbar-header">

</div>
<ul class="navbar navbar-nav navbar-right list-group">
  <li class="list-group-item"><%= link_to "Sign in", signin_path %></li>
  <li class="list-group-item"><%= link_to "Sign Up", new_user_path %></li>
</ul>
</div>
  </nav>
  <div class="container">
<% flash.each do |key,value| %>
  <div class='flash' id='<%= key %>'>
    <%= value %>
  </div>
  <% end %>
<%= yield %>
</div>
</body>
</html>
